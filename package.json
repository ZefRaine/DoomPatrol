{
  "name": "node-handlebars-boilerplate",
  "version": "1.0.0",
  "description": "Project 2 Starter",
  "main": "server.js",
  "scripts": {
    "start": "sequelize db:migrate && sequelize db:seed:all && node server.js",
    "dev": "sequelize db:migrate && sequelize db:seed:all && nodemon server.js",
    "lint": "eslint --quiet .",
    "fix": "eslint --fix .",
    "test": "cross-env NODE_ENV=test jest",
    "sass": "node-sass -w public/scss/ -o public/styles/"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "express-handlebars": "^3.1.0",
    "husky": "^3.0.7",
    "lint-staged": "^9.4.1",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "node-sass": "^7.0.0",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^1.19.3",
    "react-scripts": "^3.1.2",
    "sequelize": "^5.19.2",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.1"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --quiet .",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
